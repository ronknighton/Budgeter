
@{
    ViewBag.Title = "RemoveMember";
}
<div class="col-md-offset-3 col-md-6">
    <div class="tile">
        <h2>Remove Member/Leave Household</h2>
        @Html.ActionLink("Yes..Leave or Remove", "ConfirmRemoveMember", new { userId = ViewBag.UserId, householdId = ViewBag.HouseholdId }, new { @class = "btn-danger btn-sm" }) <br />
        @Html.ActionLink("Back to Members List", "ListMembers", "Households", new { hId = ViewBag.HouseholdId }, new { @class = "btn btn-sm" })
    </div>
</div>
